{
  "name": "ocular",
  "private": true,
  "type": "module",
  "version": "0.0.0",
  "license": "MIT",
  "author": "Simon Reinisch <contact@reinisch.io>",
  "packageManager": "pnpm@9.15.0",
  "engines": {
    "node": "^22"
  },
  "scripts": {
    "dev": "pnpm run \"/^dev:.+/\"",
    "dev:frontend": "vite --host",
    "dev:backend": "docker run --pull always -p 8080:8080 -v \"$(pwd)/.data:/app/.data\" --env-file .env.genesis ghcr.io/simonwep/genesis:latest start",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint": "eslint 'src/**/*.{js,ts,vue}' --cache",
    "lint:fix": "pnpm lint --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ci": "pnpm lint:fix && pnpm build && pnpm test"
  },
  "dependencies": {
    "@popperjs/core": "2.11.8",
    "@remixicon/vue": "4.6.0",
    "echarts": "5.6.0",
    "papaparse": "5.5.2",
    "vue": "3.5.13",
    "vue-i18n": "11.1.0",
    "vue-router": "4.5.0",
    "yuppee": "1.0.0"
  },
  "devDependencies": {
    "@eslint/js": "9.20.0",
    "@intlify/eslint-plugin-vue-i18n": "^3.2.0",
    "@types/ackee-tracker": "5.0.4",
    "@types/html-minifier-terser": "7.0.2",
    "@types/node": "22.13.1",
    "@types/papaparse": "5.3.15",
    "@typescript-eslint/parser": "8.23.0",
    "@vitejs/plugin-vue": "5.2.1",
    "@vue/eslint-config-prettier": "10.2.0",
    "@vue/eslint-config-typescript": "14.3.0",
    "eslint": "9.20.0",
    "eslint-config-prettier": "10.0.1",
    "eslint-import-resolver-typescript": "3.7.0",
    "eslint-plugin-import-x": "4.6.1",
    "eslint-plugin-prefer-arrow-functions": "3.6.2",
    "eslint-plugin-prettier": "5.2.3",
    "eslint-plugin-vue": "9.32.0",
    "html-minifier-terser": "7.2.0",
    "prettier": "3.4.2",
    "sass": "1.84.0",
    "typescript": "5.7.3",
    "typescript-eslint": "8.23.0",
    "vite": "6.1.0",
    "vite-plugin-optimize-css-modules": "1.2.0",
    "vite-plugin-pwa": "0.21.1",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.0.6",
    "vue-tsc": "2.2.0",
    "workbox-build": "7.3.0",
    "workbox-window": "7.3.0"
  }
}
