{
  "name": "ocular",
  "private": true,
  "type": "module",
  "version": "0.0.0",
  "license": "MIT",
  "author": "Simon Reinisch <trash@reinisch.io>",
  "packageManager": "pnpm@9.11.0",
  "engines": {
    "node": "^22"
  },
  "scripts": {
    "dev": "vite --host",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint": "eslint '{scripts,src}/**/*.{js,ts,vue}' --no-error-on-unmatched-pattern",
    "lint:fix": "pnpm run lint --fix",
    "test:ci": "pnpm run lint:fix && pnpm run build",
    "genesis": "docker run --pull always -p 8080:8080 -v \"$(pwd)/.data:/app/.data\" --env-file .env.genesis ghcr.io/simonwep/genesis:latest start"
  },
  "dependencies": {
    "@popperjs/core": "2.11.8",
    "@remixicon/vue": "4.2.0",
    "echarts": "5.5.1",
    "papaparse": "5.4.1",
    "vue": "3.4.31",
    "vue-i18n": "9.13.1",
    "vue-router": "4.4.0",
    "yuppee": "0.3.0"
  },
  "devDependencies": {
    "@intlify/eslint-plugin-vue-i18n": "^3.0.0",
    "@types/ackee-tracker": "5.0.4",
    "@types/node": "20.14.10",
    "@types/papaparse": "5.3.14",
    "@typescript-eslint/eslint-plugin": "7.15.0",
    "@typescript-eslint/parser": "7.15.0",
    "@vitejs/plugin-vue": "5.0.5",
    "@vue/eslint-config-prettier": "9.0.0",
    "@vue/eslint-config-typescript": "13.0.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-vue": "9.27.0",
    "prettier": "3.3.2",
    "sass": "1.77.6",
    "typescript": "5.5.3",
    "vite": "5.3.3",
    "vite-plugin-optimize-css-modules": "1.1.0",
    "vite-plugin-pwa": "0.20.0",
    "vite-tsconfig-paths": "4.3.2",
    "vue-tsc": "2.0.26",
    "workbox-build": "7.1.1",
    "workbox-window": "7.1.0"
  }
